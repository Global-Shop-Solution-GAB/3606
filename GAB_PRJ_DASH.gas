Program.Sub.subLoadPic1.Start
F.Intrinsic.Control.SetErrorHandler("subLoadPic1_Err")
F.Intrinsic.Control.ClearErrors

V.Local.sError.Declare(String)

V.Local.bExists.Declare(Boolean)
V.Local.fQtyTot.Declare(Float)
V.Local.fQtyRem.Declare(Float)
V.Local.iQtyRem.Declare(Long)
V.Local.iQtyCom.Declare(Long)
V.Local.sRet.Declare(String)
V.Local.sSeries1.Declare(String)
V.Local.sX.Declare(String)
V.Local.sY1.Declare(String)
V.Local.iX.Declare(Long)
V.Local.iY.Declare(Long)
V.Local.sPath.Declare(String)

F.Intrinsic.Math.Add(V.Global.fQtyCompleted,V.Global.fQtyRemaining,V.Local.fQtyTot)

F.Intrinsic.Control.If(V.Local.fQtyTot,>,0)
	F.Intrinsic.Math.Div(V.Global.fQtyRemaining,V.Local.fQtyTot,V.Local.fQtyRem)
	F.Intrinsic.Math.Mult(V.Local.fQtyRem,100,V.Local.fQtyRem)
	F.Intrinsic.Math.Round(V.Local.fQtyRem,0,V.Local.iQtyRem)
	F.Intrinsic.Math.Sub(100,V.Local.iQtyRem,V.Local.iQtyCom)
	
	'Create Chart -- Create a chart called "MyChart"
	F.Global.Presentation.CreateChart("MyChart",V.Local.sRet)

	'Create a Legend -- Create a legend called "MyLegend"
	F.Global.Presentation.CreateChartMember("MyChart","Legend","MyLegend",V.Local.sRet)

'	'Create a Title -- Create a Title called "MyTitle" and set the text of the title to say "My Chart Example"
	F.Global.Presentation.CreateChartMember("MyChart","Title","MyTitle","Quantity",V.Local.sRet)

	'Create Series -- Create a series called "Sales North" and get data from database
	V.Local.sX.Set("Remaining*!*Completed")
	F.Intrinsic.String.Concat(V.Local.iQtyRem,"*!*",V.Local.iQtyCom,V.Local.sY1)

	F.Intrinsic.String.Concat(V.Local.sX,"!*!",V.Local.sY1,V.Local.sSeries1)
	F.Global.Presentation.CreateChartMember("MyChart","Series","QTY","Pie",V.Local.sSeries1,V.Local.sRet)

	'Set Chart Property -- Change the chart color palette to the Pastel palette

	F.Global.Presentation.SetChartProperty("MyChart","Palette","SeaGreen",V.Local.sRet)

	'Set Chart Property -- Change the Label and Disable Chart Label
	F.Global.Presentation.SetChartProperty("MyChart","Series","QTY","Label","%#VALY #VALX",V.Local.sRet)
	F.Global.Presentation.SetChartProperty("MyChart","ChartArea","Area3DStyle:Enable3D","True",V.Local.sRet)
	F.Global.Presentation.SetChartProperty("MyChart","Series","QTY","Item:PieLabelStyle","Disabled",V.Local.sRet)

'	'Set a Title Property -- Change the font of the title to Trebuchet MS, size 20, and Bold
	F.Global.Presentation.SetChartProperty("MyChart","Title","MyTitle","Font","Trebuchet MS|14|Bold",V.Local.sRet)

	'Render --  Save to \Global\Temp\MyChart.bmp, and overwrite if file already exists
	F.Intrinsic.String.Concat(V.Caller.TempDir,"\Pic1.bmp",V.Local.sPath)
	F.Intrinsic.Math.Div(V.Screen.F_Perc!picChart1.Width,V.Ambient.TwipsPerPixelX,V.Local.iX)
	F.Intrinsic.Math.Div(V.Screen.F_Perc!picChart1.Height,V.Ambient.TwipsPerPixelX,V.Local.iY)
	F.Global.Presentation.RenderChart("MyChart",V.Local.sPath,"True",V.Local.iX,V.Local.iY,V.Local.sRet)

	'Load picture to PictureBox
	F.Intrinsic.File.Exists(V.Local.sPath,V.Local.bExists)
	F.Intrinsic.Control.If(V.Local.bExists,=,"True")
		Gui.F_Perc.picChart1.Picture(V.Local.sPath)

	'	Gui.F_Chart.picChart.Picture(V.Local.sPath)
	F.Intrinsic.Control.EndIf

	'Clear Chart -- Necessary for Re-Rendering
	F.Global.Presentation.ClearChart("MyChart",V.Local.sRet)


F.Intrinsic.Control.EndIf

F.Intrinsic.Control.ExitSub

F.Intrinsic.Control.Label("subLoadPic1_Err")
F.Intrinsic.Control.If(V.Ambient.ErrorNumber,<>,0)
	Function.Intrinsic.String.Concat("Project: GAB_PRJ_DASH.gas",V.Ambient.Newline,V.Ambient.Newline,"Subroutine: ",V.Ambient.CurrentSubroutine,V.Ambient.NewLine,"Error Occurred ",V.Ambient.ErrorNumber," with description ",V.Ambient.ErrorDescription,V.Local.sError)
	F.Intrinsic.Debug.SetLA(V.Local.sError)
Function.Intrinsic.Control.EndIf

Program.Sub.subLoadPic1.End

